<h1>Portfolio items</h1>
<%= @portfolio_items.inspect %>
<% @portfolio_items.each do |portfolio_item| %>
  <div>
    <p><%= portfolio_item.title %></p>
    <p><%= portfolio_item.subtitle %></p>
    <p><%= portfolio_item.body %></p>
    <div>
      <%= image_tag(portfolio_item.main_image, alt: portfolio_item.title) %>
    </div>
    <div>
      <%= image_tag(portfolio_item.thumb_image, alt: portfolio_item.title) %>
    </div>
  </div>
<% end %>  